{
  "name": "weather-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run serve:client\" \"npm run serve:server\"",
    "serve:client": "ng serve --proxy-config proxy.conf.json",
    "serve:server": "tsx watch server/index.ts",
    "server": "tsx server/index.ts",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
