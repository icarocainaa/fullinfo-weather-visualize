    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@apollo/client": "^4.0.1",
    "@types/leaflet": "^1.9.21",
    "apollo-angular": "^12.0.0",
    "cross-fetch": "^4.1.0",
    "d3-scale": "^4.0.2",
    "graphql": "^16",
    "graphql-yoga": "^5.16.0",
    "leaflet": "^1.9.4",
